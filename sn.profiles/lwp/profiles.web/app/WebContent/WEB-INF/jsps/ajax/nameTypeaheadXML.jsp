<%@ taglib prefix="c" 			uri="http://java.sun.com/jsp/jstl/core" %>
<%@ taglib prefix="fmt" 		uri="http://java.sun.com/jsp/jstl/fmt" %>
<%@ taglib prefix="fn" 			uri="http://java.sun.com/jsp/jstl/functions" %>
<%@ taglib prefix="html" 		uri="http://jakarta.apache.org/struts/tags-html" %>
<%@ taglib prefix="tiles" 		uri="http://jakarta.apache.org/struts/tags-tiles" %>
<%@ taglib prefix="profiles"	uri="/WEB-INF/tlds/profiles.tld" %>
<%@ taglib prefix="tags"		uri="/WEB-INF/tags" %>

<results>
<c:forEach items="${profiles}" var="profile">
	<entry>
		<uid><c:set var="uid" value="${profile.uid}" /> ${uid} </uid> 
		<displayName><c:set var="displayName" value="${profile.displayName}" /> ${displayName} </displayName> 
		<c:if test="${showEmail}"><email><c:set var="email" value="${profile.email}" /> ${email} </email></c:if>
	</entry>
</c:forEach>
</results>
