# number of orgs
# each org requires baseUrl, baseUrlHttp and username/password/email for "user", "other", "tertiary" and "orgAdmin" users
numOrgs = 2

# The URL for the Profiles server
# mtMode: org0 base url
#baseUrl=https://eed00.rtp.raleigh.ibm.com:9443
baseUrl=https://org.0.host.com:9443

# The non-SSL URL for the Profiles server - if SSL is required in the deployment, this value should be the same as baseUrl
# mtMode org0 http base url
#baseUrlHttp=https://eed00.rtp.raleigh.ibm.com:9443
baseUrlHttp=https://org.0.host.com:9443

# The user and password for the primary user to use when exercising tests
