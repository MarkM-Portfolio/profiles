<component name="profiles.sn.install"
	type="source" 
	description="Profiles Product CD Files" 
	spec-version="1.0.0" 
    >

    <output-group name="default">
        <output type="jar" visibility="public" platform="all" location="${env.LWP_COMP_BUILD}/stage3/profiles_cd.jar" preferredInstallLocation="lib"/>
    </output-group>

    <output-group name="default">
        <output type="dir" visibility="public" platform="all" location="${env.LWP_COMP_BUILD}/db.sql" preferredInstallLocation="lib"/>
    </output-group>

    <output-group name="default">
        <output type="dir" visibility="public" platform="all" location="${env.LWP_COMP_BUILD}/db.sql/profiles" preferredInstallLocation="migrate.lib"/>
    </output-group>

    <dependency name="profiles.ear" type="ear" version="3.0"/>
    <dependency name="profiles.db/schemas" type="ear" version="3.0"/>
    <dependency name="profiles.migrate" type="jar" version="3.0"/>
</component>
