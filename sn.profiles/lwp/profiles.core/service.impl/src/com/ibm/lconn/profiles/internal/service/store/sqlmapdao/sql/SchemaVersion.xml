<sqlMap namespace="Schema">

	<resultMap id="versionResult" class="java.util.HashMap">
		<result property="dbSchemaVer" column="DBSCHEMAVER" javaType="java.lang.Integer"/>
		<result property="postSchemaVer" column="POSTSCHEMAVER" javaType="java.lang.String"/>
	</resultMap>
	
	<select id="readSchemaVersion" resultMap="versionResult" parameterClass="string">
		SELECT DBSCHEMAVER, POSTSCHEMAVER from EMPINST.SNPROF_SCHEMA where COMPKEY = #appName#
	</select>
	
	<select id="SelectSchemaVersion" resultClass="int" parameterClass="string">
		SELECT DBSCHEMAVER from EMPINST.SNPROF_SCHEMA where COMPKEY = #appName#
	</select>
	
	

</sqlMap>
